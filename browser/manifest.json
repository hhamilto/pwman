{

  "manifest_version": 2,
  "name": "hurricane password manager",
  "version": "1.0",
  "description": "lololol",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "pwman",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ]
}